{
    "Type": "AWS::IAM::Policy",
    "Properties": {
        "PolicyName": "Policy Default",
        "PolicyDocument": {
            "Version": "2012-10-17",
            "Statement": [
                {
                    "Effect": "Allow",
                    "Action": [
                        "ec2:DescribeAccountAttributes",
                        "ec2:DescribeAddresses",
                        "servicequotas:RequestServiceQuotaIncrease",
                        "servicequotas:ListRequestedServiceQuotaChangeHistory"
                    ],
                    "Resource": "*"
                }
            ]
        }
    }
}
